programa {
  funcao inicio() {
    inteiro valorUm
    inteiro valorDois
    escreva("digite o valor: ")
    leia (valorUm)
    leia(valorDois)
    inteiro soma = (valorUm + valorDois)
    escreva("o valor da soma é:" + soma + "\n")
    inteiro subtrair = (valorUm - valorDois)
    escreva("a subtracao dos valores sao: " + subtrair + "\n")
    inteiro multiplicar = (valorUm * valorDois)
    escreva("o resultado da multiplicacao é: " + multiplicar + "\n")

  }
}
